{
  "name": "vuestrap-client-app-user",
  "version": "0.0.10",
  "description": "App User authorization and account/profile management for Vuestack client apps",
  "author": "|M| <manicprone@gmail.com>",
  "main": "dist/plugin.js",
  "scripts": {
    "build-plugin": "cross-env NODE_ENV=build rollup -c",
    "prepublish": "npm run build-plugin",
    "serve": "cross-env NODE_ENV=development rollup -c test/simulator/rollup.vue-dev.config.js -w",
    "dev": "npm run build-plugin && npm run serve",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "flint": "eslint ./ --fix --cache --ignore-pattern .gitignore",
    "test": "cross-env NODE_ENV=test mocha --require babel-register test/**/*.spec.js",
    "precommit-message": "echo 'Pre-commit checks (linting, building plugin)...' && exit 0"
  },
  "pre-commit": [
    "precommit-message",
    "lint",
    "build-plugin"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "joint-normalizer": "0.0.4",
    "lodash": "4.17.21",
    "vue-click-outside": "^1.0.7"
  },
